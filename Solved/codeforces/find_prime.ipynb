    "def find_prime_upto(n):\n",
    "    prime_arr = [True for i in range(n)]\n",
    "    prime_arr[0]=False\n",
    "    for i in range(2,int(n**0.5)+1):\n",
    "        if prime_arr[i-1]:\n",
    "            for j in range(i**2,n+1,i):\n",
    "                prime_arr[j-1]=False\n",
    "    return [i+1 for i in range(n) if prime_arr[i]]"
   ]
  }
